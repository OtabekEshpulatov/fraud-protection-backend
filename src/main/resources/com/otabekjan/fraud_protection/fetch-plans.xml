<fetchPlans xmlns="http://jmix.io/schema/core/fetch-plans">

    <fetchPlan name="latest-posts" entity="Post" extends="_base">
        <property name="medias"/>
        <property name="region"/>
    </fetchPlan>

    <fetchPlan name="post-apns-plan" entity="Post" extends="_base">
        <property name="medias" fetchPlan="_local"/>
        <property name="user" fetchPlan="_local"/>
    </fetchPlan>


</fetchPlans>